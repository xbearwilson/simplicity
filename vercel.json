{
	"headers": [
		{
			"source": "/(.*).html",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=0, must-revalidate"
				}
			]
		},
		{
			"source": "/(.*).(jpg|jpeg|png|gif|svg|ico|webp|js|css|woff|woff2)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		}
	]
}
